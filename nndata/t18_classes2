C fof(t18_classes2, axiom,  (! [A] :  (v1_card_1(A) =>  (! [B] :  (r2_hidden(A, k1_classes1(B)) => r1_tarski(A, k1_classes1(B))) ) ) ) ).
+ fof(cc1_card_1, axiom,  (! [A] :  (v1_card_1(A) => v3_ordinal1(A)) ) ).
+ fof(t6_classes2, axiom,  (! [A] :  (v3_ordinal1(A) =>  (! [B] :  (r2_hidden(A, k1_classes1(B)) =>  (r2_hidden(k1_ordinal1(A), k1_classes1(B)) & r1_tarski(A, k1_classes1(B))) ) ) ) ) ).
- fof(d4_classes1, axiom,  (! [A] :  (! [B] :  (B=k1_classes1(A) <=>  (r1_classes1(A, B) &  (! [C] :  (r1_classes1(A, C) => r1_tarski(B, C)) ) ) ) ) ) ).
- fof(d3_classes1, axiom,  (! [A] :  (! [B] :  (r1_classes1(A, B) <=>  (r2_hidden(A, B) & v2_classes1(B)) ) ) ) ).
