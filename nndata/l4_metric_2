C fof(l4_metric_2, axiom,  (! [A] :  ( ( ~ (v2_struct_0(A))  &  (v6_metric_1(A) & l1_metric_1(A)) )  =>  (! [B] :  (m1_subset_1(B, u1_struct_0(A)) => r2_metric_2(A, B, B)) ) ) ) ).
+ fof(reflexivity_r2_metric_2, axiom,  (! [A, B, C] :  ( ( ( ~ (v2_struct_0(A))  &  (v6_metric_1(A) & l1_metric_1(A)) )  &  (m1_subset_1(B, u1_struct_0(A)) & m1_subset_1(C, u1_struct_0(A))) )  => r2_metric_2(A, B, B)) ) ).
- fof(redefinition_r2_metric_2, axiom,  (! [A, B, C] :  ( ( ( ~ (v2_struct_0(A))  &  (v6_metric_1(A) & l1_metric_1(A)) )  &  (m1_subset_1(B, u1_struct_0(A)) & m1_subset_1(C, u1_struct_0(A))) )  =>  (r2_metric_2(A, B, C) <=> r1_metric_2(A, B, C)) ) ) ).
