C fof(t5_hallmar1, axiom,  (! [A] :  (v1_finset_1(A) =>  (! [B] :  (m2_finseq_1(B, k1_zfmisc_1(A)) =>  (! [C] :  (m1_subset_1(C, k5_numbers) =>  (r2_hidden(C, k4_finseq_1(B)) => k1_hallmar1(A, B, k1_tarski(C))=k1_funct_1(B, C)) ) ) ) ) ) ) ).
+ fof(d1_hallmar1, axiom,  (! [A] :  (! [B] :  (m2_finseq_1(B, k1_zfmisc_1(A)) =>  (! [C] :  (! [D] :  (D=k1_hallmar1(A, B, C) <=>  (! [E] :  (r2_hidden(E, D) <=>  (? [F] :  (r2_hidden(F, C) &  (r2_hidden(F, k4_finseq_1(B)) & r2_hidden(E, k1_funct_1(B, F))) ) ) ) ) ) ) ) ) ) ) ).
+ fof(d1_tarski, axiom,  (! [A] :  (! [B] :  (B=k1_tarski(A) <=>  (! [C] :  (r2_hidden(C, B) <=> C=A) ) ) ) ) ).
- fof(d3_tarski, axiom,  (! [A] :  (! [B] :  (r1_tarski(A, B) <=>  (! [C] :  (r2_hidden(C, A) => r2_hidden(C, B)) ) ) ) ) ).
