C fof(t13_classes2, axiom,  (! [A] :  (! [B] :  ( (v2_classes1(A) & r2_hidden(B, k1_card_1(A)))  => r2_hidden(B, A)) ) ) ).
+ fof(d9_ordinal1, axiom,  (! [A] :  (! [B] :  (B=k2_ordinal1(A) <=>  (! [C] :  (r2_hidden(C, B) <=>  (r2_hidden(C, A) & v3_ordinal1(C)) ) ) ) ) ) ).
+ fof(t9_classes2, axiom,  (! [A] :  (v2_classes1(A) => k2_ordinal1(A)=k1_card_1(A)) ) ).
- fof(d3_classes1, axiom,  (! [A] :  (! [B] :  (r1_classes1(A, B) <=>  (r2_hidden(A, B) & v2_classes1(B)) ) ) ) ).
- fof(d4_classes1, axiom,  (! [A] :  (! [B] :  (B=k1_classes1(A) <=>  (r1_classes1(A, B) &  (! [C] :  (r1_classes1(A, C) => r1_tarski(B, C)) ) ) ) ) ) ).
