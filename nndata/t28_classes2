C fof(t28_classes2, axiom,  (! [A] : r1_tarski(k4_classes1(k1_card_1(k1_classes1(A))), k1_classes1(A))) ).
+ fof(t27_classes2, axiom,  (! [A] :  (v2_classes1(A) => r1_tarski(k4_classes1(k1_card_1(A)), A)) ) ).
+ fof(fc1_classes2, axiom,  (! [A] : v2_classes1(k1_classes1(A))) ).
- fof(t14_classes2, axiom,  (! [A] :  (! [B] :  (r2_hidden(A, k1_card_1(k1_classes1(B))) => r2_hidden(A, k1_classes1(B))) ) ) ).
- fof(d3_classes1, axiom,  (! [A] :  (! [B] :  (r1_classes1(A, B) <=>  (r2_hidden(A, B) & v2_classes1(B)) ) ) ) ).
