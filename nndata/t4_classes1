C fof(t4_classes1, axiom,  (! [A] :  (! [B] :  (r2_hidden(A, k1_classes1(B)) => r2_hidden(k9_setfam_1(A), k1_classes1(B))) ) ) ).
+ fof(d4_classes1, axiom,  (! [A] :  (! [B] :  (B=k1_classes1(A) <=>  (r1_classes1(A, B) &  (! [C] :  (r1_classes1(A, C) => r1_tarski(B, C)) ) ) ) ) ) ).
+ fof(d3_classes1, axiom,  (! [A] :  (! [B] :  (r1_classes1(A, B) <=>  (r2_hidden(A, B) & v2_classes1(B)) ) ) ) ).
+ fof(d2_classes1, axiom,  (! [A] :  (v2_classes1(A) <=>  (v1_classes1(A) &  ( (! [B] :  (r2_hidden(B, A) => r2_hidden(k9_setfam_1(B), A)) )  &  (! [B] :  ~ ( (r1_tarski(B, A) &  ( ~ (r2_tarski(B, A))  &  ~ (r2_hidden(B, A)) ) ) ) ) ) ) ) ) ).
- fof(t2_classes1, axiom,  (! [A] : r2_hidden(A, k1_classes1(A))) ).
- fof(t3_classes1, axiom,  (! [A] :  (! [B] :  (! [C] :  ( (r2_hidden(A, k1_classes1(B)) & r1_tarski(C, A))  => r2_hidden(C, k1_classes1(B))) ) ) ) ).
