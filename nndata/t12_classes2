C fof(t12_classes2, axiom,  (! [A] :  (! [B] :  (r2_hidden(A, k1_classes1(B)) => r2_hidden(k1_card_1(A), k1_classes1(B))) ) ) ).
+ fof(fc1_classes2, axiom,  (! [A] : v2_classes1(k1_classes1(A))) ).
+ fof(t11_classes2, axiom,  (! [A] :  (! [B] :  ( (v2_classes1(A) & r2_hidden(B, A))  => r2_hidden(k1_card_1(B), A)) ) ) ).
- fof(d3_classes1, axiom,  (! [A] :  (! [B] :  (r1_classes1(A, B) <=>  (r2_hidden(A, B) & v2_classes1(B)) ) ) ) ).
- fof(d4_classes1, axiom,  (! [A] :  (! [B] :  (B=k1_classes1(A) <=>  (r1_classes1(A, B) &  (! [C] :  (r1_classes1(A, C) => r1_tarski(B, C)) ) ) ) ) ) ).
