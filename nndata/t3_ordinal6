C fof(t3_ordinal6, axiom,  (! [A] :  (v1_ordinal6(A) => r1_tarski(A, k3_ordinal2(A))) ) ).
+ fof(t2_ordinal6, axiom,  (! [A] :  (v1_ordinal6(A) <=> k2_ordinal1(A)=A) ) ).
